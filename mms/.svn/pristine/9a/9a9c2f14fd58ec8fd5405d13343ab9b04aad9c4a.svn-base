#@layout()
#define main()
<head>
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8"/>
<title>个人信息</title>
<link href="#(tp)/public/css/detail.css" rel="stylesheet" type="text/css" />
<link href="#(tp)/public/css/basic.css" rel="stylesheet" type="text/css" />
</head>

<body>
<!-- <div style="width: 100% ;height:5px"></div> -->
<table class="searchtb">
 
    <tr class="searchtr1">
      <td class="searchtd1"></td>
      <td class="searchtd2">
      	<img src="#(tp)/public/images/scbj-icon.png" style="width:16px; height:16px; margin-bottom:-2px;"/>系统管理-用户管理-用户详情
      </td>
      <td class="searchtd3"></td>
    </tr>
    <tr>
      <td colspan="3"><table class="datatb"  cellpadding="3" cellspacing="1" >
          <tbody>
            <tr class="listth" >
            <td colspan="2" align="center"><input type="button" value="返回" class="back" onclick ="history.go(-1);"/></td>
            </tr>
        	
              <tr class="listtr" >
		    <td width="20%" class="editleft">用户名:</td>
		    <td width="80%" class="editright">#(session.suser.customer_name)
		    </td>
        </tr>
        <tr  class="listtr" >
		    <td class="editleft">用户类型:</td>
		    <td class="editright">
              	#if(session.suser.customer_type == 1)技维用户#end
              	#if(session.suser.customer_type == 4)客户使用者#end
              	#if(session.suser.customer_type == 5)代理商#end
              	#if(session.suser.customer_type == 9)超级管理员#end
		    </td>
        </tr>
        <tr  class="listtr" >
		    <td class="editleft">用户所属商家:</td>
		    <td class="editright">#(session.suser.business_name)
		    </td>
        </tr>
        <tr  class="listtr" >
		    <td class="editleft">联系电话:</td>
		    <td class="editright">#(session.suser.telephone)</td>
        </tr >
        <tr  class="listtr" >
		    <td class="editleft">联系手机:</td>
		    <td class="editright">#(session.suser.mobile)</td>
        </tr>
        <tr  class="listtr" >
		    <td class="editleft">用户邮箱:</td>
		    <td class="editright">#(session.suser.email)</td>
        </tr>
        <tr  class="listtr" >
		    <td class="editleft">备注:</td>
		    <td class="editright">#(session.suser.attachment)</td>
        </tr>
        <tr  class="listtr" >
		    <td class="editleft">帐号状态:</td>
		    <td class="editright">
                	#if(session.suser.status == 0)无效#end
                	#if(session.suser.status == 1)有效#end
			</td>
        </tr>
            
          </tbody>
        </table>
        </td>
    </tr>
</table>
<!-- <%@include file="/public/files/loading.jsp"%>  -->
</body>
</html>
#end