/*广告信息*/
#ad-xinx{margin:26px 30px;border-radius:4px;border:1px solid #1a85c1;overflow:hidden;font-size:14px;color:#5a5a5a;}
#ad-xinx h2{background:#e8f3f9;height:27px;line-height:27px;font-size:14px;font-weight:bold;color:#5a5a5a;padding-left:260px;}

/* 分页 */
#fanye{height:38px;line-height:38px; clear:both;width:95%; margin:10px auto; }
#fanye .left{float:left;font-size:14px;color:#4b4b4b;}
#fanye .left font{color:#F00;padding:0 5px;}
#fanye .right{float:right;font-size:12px;}
#fanye .right .ym{float:left;}
#fanye .right .ym ul{border-right:1px solid #ddd;float:left;margin-right:5px;}
#fanye .right .ym li{float:left;border:1px solid #ddd;border-right:0;line-height:34px;}
#fanye .right .ym li a{display:block;text-decoration:none;padding:0 15px;color:#666;}
#fanye .right .ym li a:hover{background:#ddd;}
#fanye .right .sy{margin-right:5px;float:left;border:1px solid #ddd;line-height:34px;text-align:center;width:72px;}
#fanye .right .sy a{text-decoration:none;color:#666;display:block;}
#fanye .right .sy a:hover{background:#ddd;}
#fanye .right .ts{float:left;line-height:34px;font-size:12px;}
#fanye .right .ts input{border:1px solid #ddd;width:30px;text-align:center;margin:0 3px;height:20px;}
#fanye .right .ts span{display:block;padding-top:1px;float:right;}
#fanye .right .ts span a{margin:1px 0 0 10px;border:1px solid #ddd;text-align:center;padding:2px 10px 3px 10px;text-decoration:none;color:#666;}
#fanye .right .ts span a:hover{background:#1a85c1;color:#fff;}


