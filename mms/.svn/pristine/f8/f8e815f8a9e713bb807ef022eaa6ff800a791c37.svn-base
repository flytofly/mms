/* 帮助 */
.relative {
    position: relative;
    left: 0;
    top: 0;
    z-index: 2
}

a.bottun4 font {
    color: #ffffff;
    display: block;
    height: 30px;
	border-left: 1px solid #7cb9db;
    padding: 0 0px 0 13px;
    font-size: 13px;
    float: left
}

a.bottun4, a:visited.bottun4 {
    display: block;
    text-decoration: none;
    height: 30px;
    line-height: 29px;
    padding-right: 15px;
    margin-top: 5px;
    color: #2e323b;
    float: left
}

.server {
    position: absolute;
    right: 5px;
    top: 35px;
    background: url(../images/server_bg_bl.gif) no-repeat left bottom;
    display: none;
    opacity: .95;
    filter: alpha(opacity=95);
    z-index: 1000;
    width: 470px;
}

.Exp .server {
    right: -150px;
    top: 17px;
}

.l {
    float: left
}

.r {
    float: right
}

.ser_title {
    height: 38px;
    line-height: 38px;
    background: url(../images/server_bg_tr.gif) no-repeat right top;
    width: 100%;
    color: #fff
}
.Exp .server .ser_title{
    background: url(../images/server_bg_tr_exp.gif) no-repeat right top;
}

.ser_title a {
    margin: 15px 15px 0 0;
    height:10px;
}

.ser_title b {
    display: block;
    height: 38px;
    background: url(../images/server_bg_tl.gif) no-repeat left 6px;
    padding-left: 10px;
    font-size: 14px
}

.ser_txt {
    background: url(../images/server_bg_br.gif) no-repeat right bottom;
    width: 100%;
    padding: 0 0 0px 0;
    margin: 0;
}

.ser_txt dl {
    background: #4f5153;
    padding: 10px 10px;
    overflow: hidden;
    max-height:280px;
}

.ser_txt a, .ser_txt a:visited {
    color: #28a7e1
}

.ser_txt dt, .ser_txt dd {
    background: #4f5153;
    padding: 5px 5px 5px 5px;
    line-height: 22px;
    color: #fff;
    text-align: left
}
.ser_txt dt:after, .ser_txt dd:after{
    display: block;
    clear: both;
    content: '';
    height: 0;
}

.ser_txt dd {
    background: #67696b
}

.ser_txt dt font, .ser_txt dd font {
    margin-right: 20px;
    display: block;
    float: left;
    width: 90px;
}

.ser_txt dt small, .ser_txt dd small {
    font-size: 12px;
    display: block;
    float: left;
    max-width: 310px;
}

