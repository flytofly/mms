    <script>
    $(function(){
        var ruleInfo = $('#ruleInfo');
        var ruleTabs = ruleInfo.find('.tab li');
        var sTabs = ruleInfo.find('.search-tab li');
        var tags = ruleInfo.find('.search-result .tags');
        var tabCon = ruleInfo.find('.tab-con');
            ruleTabs.click(function(){
                var $this = $(this);
                var ind = $this.index();
                    $this.addClass('on').siblings().removeClass('on');
                    tabCon.eq(ind).show().siblings('.tab-con').hide();
            });

            //搜索结果 相关推荐
            sTabs.click(function(){
                var $this = $(this);
                var ind = $this.index();
                    $this.addClass('on').siblings().removeClass('on');
                    tags.eq(ind).show().siblings('.tags').hide();
            });


        //时间轴
        var investAmounts = [1,3,5,7,30,60,90,180];
        //拖动
        var yell_s_width = $('.jindu-blue').width();
        var s_width = $('#amountSpan').width();
        var jindu_icon_width = $('#jindu-icon').width() / 2;
        function setAmount(left){
            var index = Math.round(left / (s_width/8));
            var left = index * (s_width/8)-5;
            if(index == 0){
                left -= 10;
            }
            else if(index == 5){
                left += 10;
            }
            else if(index == 6){
                left += 10;
            }
            else if(index == 7){
                left += jindu_icon_width;
            }
            console.log(index)
            //console.log(left)
            
            $('.jindu-blue').css("width", left);
            $('#jindu-icon').css("left", left);
            $('#jindu-icon').data('amount', investAmounts[index]);
            //calcIncome();
        }
        setAmount( $('.jindu-blue').width());
        $('#jindu-icon').drag({
            before: function(e) {
                // $(this).text('拖动前' + e.pageX);  
            },
            process: function(e) {
                // document.title = '拖动中' + e.pageY; 
                var position = $('#jindu-icon').position(); //获取p元素的position()  
                var left = position.left;
                $('.jindu-blue').css("width", left);
            },
            end: function(e) {
                var position = $('#jindu-icon').position(); //获取p元素的position()  
                var left = position.left;
                setAmount(left);
                
            }
        });
    })
    </script>
